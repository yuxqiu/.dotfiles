[core]
	excludesfile = ~/.gitignore_global
[commit]
	template = ~/.gitmessage
	gpgsign = true
[gpg]
	program = /opt/homebrew/bin/gpg
[init]
    defaultBranch = main
[alias]
	graph = log --all --graph --decorate --oneline
	create = branch
